class Solution {
    public int[] countBits(int n) {
        int[] ans=new int[n+1];
        int a=0;
        for(int i=0;i<n+1;i++){
            String s=Integer.toBinaryString(i);
            for(int j=0;j<s.length();j++){
                if(s.charAt(j)-'0'==1){
                    a++;
                }
            }
            ans[i]=a;
            a=0;
        }
        return ans;
    }
}
