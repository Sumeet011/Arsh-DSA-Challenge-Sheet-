class Solution {
    public boolean backspaceCompare(String s, String t) {
         ArrayList<Character> charList = new ArrayList<>();
          ArrayList<Character> charList1 = new ArrayList<>();
        for(int i=0;i<Math.max(s.length(),t.length());i++){
              if(i<s.length() && s.charAt(i)=='#'){
                if(charList.size()!=0){
                charList.remove(charList.size()-1);
                }
              }
              else if(i<s.length()){
                charList.add(s.charAt(i));
              }
              if(i<t.length() && t.charAt(i)=='#'){
                if(charList1.size()!=0){
                charList1.remove(charList1.size()-1);
                }
              }
              else if(i<t.length()){
                charList1.add(t.charAt(i));
              }
        }
        return (charList.toString().equals(charList1.toString()))?true : false;
    }
}
