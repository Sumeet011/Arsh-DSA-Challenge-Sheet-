class Solution {
    public int majorityElement(int[] nums) {
        Arrays.sort(nums);
        int time=1;
        for(int i=0;i<nums.length-1;i++){
            if(nums[i]==nums[i+1]){
                time++;
            }else{
                if(time>nums.length/2){
                    return nums[i];
                }
                time=1;
            }
        }
        return nums[nums.length-1];
        
    }
}
