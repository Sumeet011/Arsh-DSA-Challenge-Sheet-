class Solution {
    public ListNode deleteDuplicates(ListNode head) {
        if (head == null) {
            return null; 
        }

        ListNode current = head;
        int[] occurrencep = new int[101]; 
        int[] occurrencen = new int[101]; 
        ListNode dummy = new ListNode(0); 
        ListNode tail = dummy; 

        while (current != null) {
            if(current.val>=0){
            if (occurrencep[current.val] == 0) {
                occurrencep[current.val]++;
                tail.next = new ListNode(current.val); 
                tail = tail.next; 
            }
            }
            else{
                if (occurrencen[Math.abs(current.val)] == 0) {
                occurrencen[Math.abs(current.val)]++;
                tail.next = new ListNode(current.val); 
                tail = tail.next; 
            }

            }
            current = current.next; 
        }

        return dummy.next; 
    }
}
