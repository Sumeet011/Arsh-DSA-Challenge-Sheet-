class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        int[] a=new int[100001];
        ArrayList<Integer> ans=new ArrayList<>();
        for(int i=0;i<nums.length;i++){
            a[nums[i]]++;
            if(a[nums[i]]==2){
                ans.add(nums[i]);
                a[nums[i]]=-1000;
            }
        }
        return ans;
    }
}
