class Solution {
    public int removeDuplicates(int[] nums) {
        if (nums.length == 1) {
            return 1;
        }
        int n = nums[0];
        int index = 1;
        int n1 = 1;
        for (int i = 1; i < nums.length; i++) {
            if (nums[i] != n) {
                n = nums[i];
                nums[index] = nums[i];
                index++;
                n1++;
            }
        }
        return n1;

    }
}
